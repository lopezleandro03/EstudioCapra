@model EstudioCapra.Models.ObjetoMultimediaModel

<div class="portlet-title">
    <div class="caption">
        <i class="fa fa-gift"></i>Nuevo Objeto Multimedia
    </div>
</div>
<div class="portlet-body form">
    <!-- BEGIN FORM-->
    <form method="post" asp-action="Create" asp-controller="ObjetoMultimedia" enctype="multipart/form-data">
        <div class="form-body">
            <div class="form-group">
                <label asp-for="Nombre" class="control-label"></label>
                <input asp-for="Nombre" class="form-control" placeholder="Nombre Archivo">
            </div>
            <div class="form-group">
                <label asp-for="Archivo" class="control-label"></label>
                <input asp-for="Archivo" class="form-control">
            </div>
            <input id="ObjMultimediaModelEtapaId" hidden asp-for="EtapaId" />
            <input id="ObjMultimediaModelContratoId" hidden asp-for="ContratoId" />
        </div>
        <div class="form-actions">
            <button type="submit" class="btn green">Submit</button>
            <button type="button" id="btnCancelNuevoObjMultimedia" class="btn default">Cancel</button>
        </div>
    </form>
    <!-- END FORM-->
</div>

<script>
    $(function () {
        $("#ObjMultimediaModelEtapaId").val($("#etapaId").val());
        $("#ObjMultimediaModelContratoId").val($("#contratoId").val());
    });

    $("#btnCancelNuevoObjMultimedia").click(function () {
        $("#modalNuevoObjMultimedia").modal('toggle');
    })
</script>